{
  "name": "@resumematch/auth",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome check .",
    "format": "biome format --write .",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@resumematch/db": "workspace:*",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:"
  }
}
